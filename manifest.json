{
  "manifest_version": 3,
  "version": "1.0",
  "name": "UUID v4 Generator",
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_icon": "icon.png",
    "default_title": "UUID v4 Generator"
  },
  "permissions": ["offscreen", "clipboardWrite"]
}

